{
  "name": "react-form-for",
  "version": "1.1.0",
  "description": "A simple form builder for React in the style of Rails' form_for",
  "main": "./lib/index.js",
  "author": {
    "name": "James Friend",
    "email": "james@jsdf.co"
  },
  "license": "MIT",
  "homepage": "https://github.com/jsdf/react-form-for",
  "bugs": {
    "url": "https://github.com/jsdf/react-form-for/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jsdf/react-form-for.git"
  },
  "keywords": [
    "react",
    "form-builder",
    "react-component",
    "forms"
  ],
  "scripts": {
    "test": "npm run prepublish && jest",
    "jest": "jest",
    "watch": "node tasks/watch test src/ example/",
    "demo": "npm run prepublish && mkdir -p example/output/ && browserify -t [ reactify --harmony ] example/demo.js > example/output/bundle.js && cp ./node_modules/bootstrap/dist/css/bootstrap.css example/output/",
    "demo-run": "open http://0.0.0.0:8080/ && http-server example/",
    "umd": "browserify ./lib/index.js --external react/addons --standalone ReactFormFor > dist/react-form-for.js",
    "dist-umd": "npm run umd; git add dist/; git commit -m 'umd distribution rebuild'",
    "dist-version": "npm version $V; git tag -d v$V; bower version $V; git push origin --tags",
    "dist": "npm run dist-umd; echo 'version:'; read version; V=$version npm run dist-version; git push origin master",
    "prepublish": "babel --experimental src/ --out-dir lib/"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest-preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules"
    ]
  },
  "dependencies": {
    "classnames": "^1.1.4",
    "lodash.memoize": "^2.4.1",
    "standard-error": "^1.1.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "react": "^0.12.2",
    "babel": "^4.5.4",
    "browserify": "^4.2.3",
    "chokidar": "^0.11.1",
    "jest-cli": "^0.1.18",
    "jquery": "^2.1.1",
    "js-beautify": "^1.5.4",
    "object.assign": "^1.0.1",
    "reactify": "^0.17.0"
  },
